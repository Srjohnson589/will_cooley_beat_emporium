{
    "version": 2,
    "builds": [
      {
        "src": "client/package.json",  
        "use": "@vercel/static-build"  
      },
      {
        "src": "run.py",  
        "use": "@vercel/python"
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "run.py"
      },
      {
        "src": "/(.*)", 
        "dest": "client/build/$1"
      }
    ]
  }
  