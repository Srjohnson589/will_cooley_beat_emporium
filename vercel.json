{
  "version": 2,
  "builds": [
    {
      "src": "run.py",
      "use": "@vercel/python"
    }
  ], 
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "run.py"
    },
    {
      "src": "/create_checkout_session",
      "dest": "run.py"
    },
    {
      "src": "/session_status",
      "dest": "run.py"
    },
    {
      "src": "/(.*)",
      "dest": "run.py"
    }
  ]
} 
